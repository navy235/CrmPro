@model  int
@{
    string name = ViewData.TemplateInfo.GetFullHtmlFieldName("");
}
<span id="@name">
    <img src="~/Content/images/loading1.gif" /></span>

<script type="text/javascript">
    jQuery(function () {
        jQuery.ajax({
            type: 'GET',
            url: '@Url.Action(name + "Name", "AjaxService")',
            data: 'key=@Model',
            success: function (res) {
                $('#@name').html(res);
            }
        })
    })
</script>
